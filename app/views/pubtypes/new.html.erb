<h1>New pubtype</h1>

<% form_for(@pubtype) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :one %><br />
    <%= f.text_field :one %>
  </p>
  <p>
    <%= f.label :five %><br />
    <%= f.text_field :five %>
  </p>
  <p>
    <%= f.label :ten %><br />
    <%= f.text_field :ten %>
  </p>
  <p>
    <%= f.label :all %><br />
    <%= f.text_field :all %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', pubtypes_path %>
