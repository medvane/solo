<h1>Listing journals</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Abbr</th>
    <th>Issn</th>
    <th>Essn</th>
    <th>Nlm</th>
    <th>Impact factor</th>
    <th>One</th>
    <th>Five</th>
    <th>Ten</th>
    <th>All</th>
  </tr>

<% for journal in @journals %>
  <tr>
    <td><%=h journal.title %></td>
    <td><%=h journal.abbr %></td>
    <td><%=h journal.issn %></td>
    <td><%=h journal.essn %></td>
    <td><%=h journal.nlm_id %></td>
    <td><%=h journal.impact_factor %></td>
    <td><%=h journal.one %></td>
    <td><%=h journal.five %></td>
    <td><%=h journal.ten %></td>
    <td><%=h journal.all %></td>
    <td><%= link_to 'Show', journal %></td>
    <td><%= link_to 'Edit', edit_journal_path(journal) %></td>
    <td><%= link_to 'Destroy', journal, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New journal', new_journal_path %>
